{
  "expo": {
    "name": "J-Planner",
    "slug": "j-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "jplanner",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jplanner.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F0F0F"
      },
      "package": "com.jplanner.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-local-authentication",
      "expo-secure-store",
      "expo-notifications",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow J-Planner to access your camera to scan documents and add photos to your notes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow J-Planner to access your photos to add images to your notes and tasks."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}